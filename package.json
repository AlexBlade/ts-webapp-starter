{
    "name": "ts-webapp-starter",
    "version": "1.0.0",
    "description": "typescript webapp starter",
    "main": "src/backend/Backend.js",
    "scripts": {
        "backend": "ts-node src/backend/Backend.ts --config=backend.json",
        "frontend": "webpack-dev-server --env.NODE_ENV=local --env.mode=development --mode=development --open --config-name=frontend",
        "test": "mocha --require ts-node/register --recursive test/**/*.spec.ts",
        "dev": "webpack --env.NODE_ENV=local --env.mode=development --mode=development",
        "prod": "webpack --env.mode=production --mode=production",
        "dev:backend": "webpack --env.NODE_ENV=local --env.mode=development --mode=development --config-name=backend",
        "prod:backend": "webpack --env.mode=production --mode=production --config-name=backend",
        "dev:frontent": "webpack --env.NODE_ENV=local --env.mode=development --mode=development --config-name=frontend",
        "prod:frontend": "webpack --env.mode=production --mode=production --config-name=frontend"
    },
    "keywords": ["typescript webapp starter"],
    "author": "Alex Blade <alex.blade.box@gmail.com>",
    "license": "GPL",
    "devDependencies": {
        "@types/cookie-parser": "^1.4.2",
        "@types/ejs": "^3.0.4",
        "@types/express": "^4.17.6",
        "@types/mocha": "^8.0.3",
        "@types/moment": "^2.13.0",
        "@types/mongoose": "^5.7.21",
        "@types/node": "^14.0.9",
        "@types/react": "^16.9.36",
        "@types/react-dom": "^16.9.8",
        "@types/react-router-dom": "^5.1.5",
        "@types/redux": "^3.6.0",
        "@types/react-redux": "^7.1.9",
        "@types/winston": "^2.4.4",
        "css-loader": "^3.6.0",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.2.1",
        "file-loader": "^6.0.0",
        "html-webpack-plugin": "^4.3.0",
        "mocha": "^8.1.2",
        "node-sass": "^4.14.1",
        "ts-loader": "^7.0.5",
        "ts-node": "^9.0.0",
        "typescript": "^4.0.2",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.11.0"
    },
    "dependencies": {
        "@bpnet/unicon": "^0.5.4",
        "@fortawesome/fontawesome-free": "^5.13.1",
        "@overnightjs/core": "^1.7.4",
        "colors": "^1.4.0",
        "cookie-parser": "^1.4.5",
        "ejs": "^3.1.3",
        "express": "^4.17.1",
        "moment": "^2.27.0",
        "mongoose": "^5.10.6",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-redux": "^7.2.0",
        "react-router-dom": "^5.2.0",
        "redux": "^4.0.5",
        "resolve-url-loader": "^3.1.1",
        "winston": "^3.3.3",
        "winston-daily-rotate-file": "^4.5.0"
    }
}
